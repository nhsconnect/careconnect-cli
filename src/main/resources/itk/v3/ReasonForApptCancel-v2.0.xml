<?xml version="1.0" encoding="UTF-8"?>
<vocabulary name="ReasonForApptCancel" version="2.0" status="superseded"
            id="2.16.840.1.113883.2.1.3.2.4.16.18">
   <Description>
            <p>Default Scheme: NHS Data Dictionary <i>ReasonForApptCancel</i>
            </p>
            <p>Assigned OID: 2.16.840.1.113883.2.1.3.2.4.16.18</p>
            <p>A classification of why the appointment was cancelled. Generally codes 01-19 would be used in the Notify Appointment Cancellation Message, and codes 20-39 in the Request Appointment Cancellation message.</p>
         </Description>
   <concept code="01">
      <displayName>Clinic cancelled</displayName>
   </concept>
   <concept code="02">
      <displayName>Slot cancelled</displayName>
   </concept>
   <concept code="03">
      <displayName>Clinic overrun</displayName>
   </concept>
   <concept code="19">
      <displayName>By provider - reason not specified</displayName>
   </concept>
   <concept code="20">
      <displayName>Patient died</displayName>
   </concept>
   <concept code="21">
      <displayName>Patient ill</displayName>
   </concept>
   <concept code="22">
      <displayName>In holiday period</displayName>
   </concept>
   <concept code="23">
      <displayName>Other more urgent engagement</displayName>
   </concept>
   <concept code="24">
      <displayName>Appt. made for wrong patient</displayName>
   </concept>
   <concept code="25">
      <displayName>Appt. made with wrong provider</displayName>
   </concept>
   <concept code="39">
      <displayName>By patient - reason not specified</displayName>
   </concept>
   <concept code="99">
      <displayName>Not specified</displayName>
   </concept>
</vocabulary>