   </concept>
   <concept code="04">
      <displayName>Abnormality suspected with no risk factors</displayName>
   </concept>
   <concept code="09">
      <displayName>Screen declined</displayName>
   </concept>
   <concept code="98">
      <displayName>Incomplete</displayName>
